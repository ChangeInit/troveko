<h1>Experiences#index</h1>
<p>Find me in app/views/experiences/index.html.erb</p>


<!-- basic loop for index page -->

<ol>
<% @experiences.each do |experience| %>
<li>
  <%= experience.address %> |
  <%= experience.title %> |
  <%= experience.price %> |
  <%= link_to experience.user.email %> |
  <%= link_to experience.title, experience_path(experience) %>
  </li>
<% end %>
</ol>


<!-- let ngos list new experience -->

<%# if current_user.ngo? %>
        <%#= link_to "List New Experience", new_bunker_path, :class => "button" %>
<%# end %>
